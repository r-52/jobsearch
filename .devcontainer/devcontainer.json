{
    "name": "jobsearch",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "app",
    "workspaceFolder": "/workspaces",
    "remoteEnv": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "APSNETCORE_URLS":"http://*:5000,https://*:5001"
    },
    "forwardPorts": [
        5000,
        5001,
        6006
    ],
    "settings": {},
    "extensions": [
		"angular.ng-template",
		"ms-dotnettools.csharp",
		"dotjoshjohnson.xml",
		"editorconfig.editorconfig",
		"eamodio.gitlens"
	]
}
